{
    "editor.fontLigatures": true,
    "editor.fontFamily": "Fira Code",
    "editor.fontSize": 11,
    "editor.rulers": [ 80 ],
    "editor.tabSize": 2,
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "onFocusChange",
    "align.leftSeparators": [],
    "align.rightSeparators": [
      ":",
      "=",
      "+=",
      "-=",
      "*=",
      "/=",
      "?=",
      "|=",
      "%=",
      ".=",
      "=>"
    ],
    "align.spaceSeparators": [
      ":",
      "=",
      "+=",
      "-=",
      "*=",
      "/=",
      "?=",
      "|=",
      "%=",
      ".=",
      "=>"
    ],
    "window.zoomLevel": 3
  }